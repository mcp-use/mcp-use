{
  "name": "@mcp-use/mango",
  "type": "module",
  "version": "0.1.0",
  "description": "Mango Agent - AI agent that builds and tests MCP servers in E2B sandboxes",
  "author": "mcp-use, Inc.",
  "license": "MIT",
  "homepage": "https://github.com/mcp-use/mcp-use#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-use/mcp-use.git",
    "directory": "libraries/typescript/packages/mango"
  },
  "bugs": {
    "url": "https://github.com/mcp-use/mcp-use/issues"
  },
  "keywords": [
    "mcp",
    "mango",
    "agent",
    "e2b",
    "sandbox",
    "anthropic"
  ],
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "bin": {
    "mango": "./dist/server/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd src/client && vite --port 5173",
    "dev:server": "tsx watch src/server/index.ts",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsup src/server/**/*.ts --format esm --out-dir dist/server && tsc -p tsconfig.json --emitDeclarationOnly --declaration",
    "build:client": "cd src/client && vite build",
    "start": "node dist/server/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^1.0.0",
    "@hono/node-server": "^1.19.6",
    "dotenv": "^16.4.7",
    "hono": "^4.6.11",
    "streamdown": "^1.6.10"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.3.5",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
