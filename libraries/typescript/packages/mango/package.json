{
  "name": "@mcp-use/mango",
  "type": "module",
  "version": "0.1.0",
  "description": "AI agent for creating and editing MCP servers live through the inspector",
  "author": "mcp-use, Inc.",
  "license": "MIT",
  "homepage": "https://github.com/mcp-use/mcp-use#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-use/mcp-use.git",
    "directory": "libraries/typescript/packages/mango"
  },
  "bugs": {
    "url": "https://github.com/mcp-use/mcp-use/issues"
  },
  "keywords": [
    "mcp",
